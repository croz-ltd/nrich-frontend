{
  "name": "nrich-frontend",
  "author": "CROZ",
  "devDependencies": {
    "@changesets/cli": "^2.25.2",
    "syncpack": "^8.3.9",
    "turbo": "^1.6.3",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "croz",
    "nrich",
    "react",
    "typescript",
    "zustand"
  ],
  "license": "Apache-2.0",
  "packageManager": "yarn@3.2.4",
  "private": true,
  "repository": "croz-ltd/nrich-frontend.git",
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "turbo run clean",
    "lint": "turbo run lint",
    "release": "build && changeset publish",
    "test": "turbo run test",
    "version-packages": "changeset version && yarn --mode=\"update-lockfile\""
  },
  "workspaces": [
    "config/**/*",
    "libs/**/*"
  ]
}
